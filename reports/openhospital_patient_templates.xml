<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <template id="report_patient">
    <t t-call="web.html_container">
      <t t-foreach="docs" t-as="doc">
        <t t-call="web.external_layout">
          <div class="page">
            <h2>Test Print</h2>
            ID:
            <t t-esc="doc.name_seq" />
            Name:
            <t t-esc="doc.name" />
            Age:
            <t t-esc="doc.age" />
          </div>
        </t>
      </t>
    </t>
  </template>

</odoo>
